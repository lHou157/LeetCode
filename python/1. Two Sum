class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
        for  idx1,val1 in enumerate(nums):
            #for idx2,val2 in enumerate(nums):
            if idx1 < len(nums):
                for i in range(idx1+1,len(nums)):
                    if target == val1 + nums[i]:
                        answer = [idx1,i]
                
        return answer
